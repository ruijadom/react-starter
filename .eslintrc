{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["react"],
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "globals": {
    "fetch": true
  },
  "rules": {
    "comma-dangle": 0,
    "arrow-parens": 0,
    "react/jsx-filename-extension": 0
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
